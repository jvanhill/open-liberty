<hazelcast
	xsi:schemaLocation="http://www.hazelcast.com/schema/config hazelcast-config-3.9.xsd"
	xmlns="http://www.hazelcast.com/schema/config"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<group>
		<name>${hazelcast.group.name}</name>
		<password>${hazelcast.group.password}</password>
	</group>

	<network>
	    <!--  TODO Remove this or add constant. Needed this on MY machine b/c 5702 was used by some system process.  -->
		<port auto-increment="true" port-count="100">5703</port>

		<join>
			<multicast enabled="false" />
			<tcp-ip enabled="true">
				<interface>127.0.0.1</interface>
			</tcp-ip>
		</join>
	</network>

	<executor-service name="default">
		<pool-size>12</pool-size>
	</executor-service>

    <!-- Require some of this to the cache in management center. -->
    <!-- 
	<cache name="jCache1">
